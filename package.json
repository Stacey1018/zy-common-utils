{
  "name": "zy-common-utils",
  "version": "1.1.6",
  "description": "js工具库",
  "main": "dist/index.js",
  "types": "dist/typings",
  "exports": {
    ".": "./dist/lib-esm/index.js",
    "./umd": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "compile": "npx babel src/ --out-dir lib",
    "build": "rm -rf dist/** && npm run build:umd && npm run build:lib-esm",
    "build:tsc": "tsc",
    "build:umd": "webpack --config webpack.base.js",
    "build:lib-esm": "tsc -p tsconfig.json --declarationDir ./dist/typings -m es6 --outDir dist/lib-esm",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stacey1018/zy-common-utils.git#main"
  },
  "keywords": [
    "js"
  ],
  "author": "zhangyang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Stacey1018/zy-common-utils/issues"
  },
  "homepage": "https://github.com/Stacey1018/zy-common-utils/tree/main#readme",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.5",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "babel-loader": "^9.1.0",
    "eslint": "^8.31.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.20.6"
  }
}
